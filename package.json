{
	"name": "vibe",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"author": "adityabmv",
	"devDependencies": {
	  "@typescript-eslint/eslint-plugin": "^8.31.1",
	  "@typescript-eslint/parser": "^8.31.1",
	  "eslint": "^9.26.0",
	  "husky": "^9.1.7",
	  "lint-staged": "^15.5.0"
	},
	"scripts": {
	  "prepare": "husky",
	  "binaries": "pnpx ts-node scripts/preload-mongo-binary.ts",
	  "vibe": "pnpx ts-node scripts/cli/src/cli.ts",
	  "lint": "eslint .",
	  "format": "prettier --write ."
	},
	"lint-staged": {
	  "backend/**/*.{js,jsx,ts,tsx}": [
		"pnpm --filter backend lint"
	  ]
	},
	"prettier": {
	  "singleQuote": true,
	  "trailingComma": "es5",
	  "printWidth": 100,
	  "endOfLine": "lf"
	}
  }
  