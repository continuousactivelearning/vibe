{
  "name": "backend",
  "version": "3.0.0",
  "description": "This is ViBe backend.",
  "main": "index.js",
  "type": "module",
  "imports": {
    "#root/container.js": "./build/container.js",
    "#root/inversify-adapter.js": "./build/inversify-adapter.js",
    "#root/instrument.js": "./build/instrument.js",
    "#root/index.js": "./build/index.js",
    "#root/types.js": "./build/types.js",
    "#root/utils/*.js": "./build/utils/*.js",
    "#root/shared/*.js": "./build/shared/*.js",
    "#shared/*.js": "./build/shared/*.js",
    "#config/*.js": "./build/config/*.js",
    "#auth/*.js": "./build/modules/auth/*.js",
    "#courses/*.js": "./build/modules/courses/*.js",
    "#users/*.js": "./build/modules/users/*.js",
    "#quizzes/*.js": "./build/modules/quizzes/*.js",
    "#docs/*.js": "./build/modules/docs/*.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --detectOpenHandles",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --detectOpenHandles --forceExit",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --detectOpenHandles",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage  --detectOpenHandles",
    "test:watch:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --coverage --detectOpenHandles",
    "dev": "pnpx ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc && pnpx tsc-alias && node esbuild.config.js && pnpm sentry:sourcemaps",
    "start": "node build/index.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org dled --project vibe-backend ./build",
    "sentry:upload": "sentry-cli sourcemaps upload --org dled --project vibe-backend ./build",
    "docs": "typedoc",
    "lint": "pnpx gts lint",
    "clean": "pnpx gts clean",
    "compile": "tsc",
    "fix": "pnpx gts fix",
    "emulators:auth": "firebase emulators:start --only auth",
    "emulators:functions": "firebase emulators:start --only functions",
    "emulators:all": "firebase emulators:start",
    "exp-openapi": "pnpx ts-node -r tsconfig-paths/register scripts/generate-openapi.ts --stdout",
    "gen-openapi": "pnpx ts-node -r tsconfig-paths/register scripts/generate-openapi.ts"
  },
  "bin": "build/index.js",
  "pkg": {
    "assets": [
      "node_modules/firebase-admin/**/*"
    ],
    "targets": [
      "latest-win-x64"
    ],
    "outputPath": "output"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@jest/globals": "30.0.0-beta.3",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "docusaurus-plugin-typedoc": "^1.2.3",
    "esbuild-plugin-tsconfig-paths": "^1.0.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "gts": "^6.0.2",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.2",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@scalar/express-api-reference": "^0.7.6",
    "@sentry/cli": "^2.43.0",
    "@sentry/node": "^9.27.0",
    "@sentry/profiling-node": "^9.27.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.2",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^13.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "graphql": "~16.10.0",
    "inversify": "^7.5.2",
    "jsonwebtoken": "^9.0.2",
    "lexorank": "^1.0.5",
    "mathjs": "^14.4.0",
    "mongodb": "^6.15.0",
    "multer": "1.4.5-lts.1",
    "nanoid": "^5.1.2",
    "onnxruntime-node": "^1.22.0",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "routing-controllers-openapi": "github:adityabmv/routing-controllers-openapi",
    "typedi": "^0.10.0",
    "winston": "^3.17.0"
  }
}
